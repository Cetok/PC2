@model PC2.Models.Bancos

<h2>Registrar Banco</h2>

<form asp-controller="Banco" asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="NombreTitular"></label>
        <input asp-for="NombreTitular" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="TipoCuenta"></label>
        <input asp-for="TipoCuenta" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="SaldoInicial"></label>
        <input asp-for="SaldoInicial" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Registrar</button>
</form>